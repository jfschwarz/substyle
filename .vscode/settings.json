{
  "javascript.validate.enable": false,
  "editor.tabSize": 2,
  "files.exclude": {
    ".git": true,
    "**/node_modules": true,
    "**/lib": true,
    "**/es6": true,
    "**/reports": true,
    "**/build": true
  },
  "search.exclude": {
